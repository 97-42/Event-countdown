{
  "manifest_version": 3,
  "name": "Countdown Extension",
  "version": "1.0",
  "description": "Countdown to specific dates.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
